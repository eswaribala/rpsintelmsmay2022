{
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft.AspNetCore": "Warning"
        }
    },
    "AllowedHosts": "*",
    "spring": {
        "application": {
            "name": "Order-API"
        }
    },

    "eureka": {
        "client": {
            "serviceUrl": "http://localhost:8761/eureka/",
            "shouldRegisterWithEureka": true,
            "shouldFetchRegistry": false,
            "registryFetchIntervalSeconds": 10,
            "instanceInfoReplicationIntervalSeconds": 10

        },
        "instance": {
            "hostname": "localhost",
            "port": 7074
        }
    }
}
